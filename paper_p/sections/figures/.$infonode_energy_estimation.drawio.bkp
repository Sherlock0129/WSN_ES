                <mxCell id="check1" value="是否存在信息滞后?&#10;t - t_last_update &gt; 0" style="rhombus;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;strokeWidth=2;fontSize=10;align=center" parent="1" vertex="1">
